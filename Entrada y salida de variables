nombre= str(input('ingrese su nombre: '))
ciudad= str(input('ciudad: '))

from datetime import date 
from datetime import datetime

y = date.today().year
print(y)

a単o= int(input('a単o de nacimiento: '))
edad= y - a単o

edad_en_meses= edad * 12

print(f"{nombre} su edad es {edad} a単os o {edad_en_meses} meses")